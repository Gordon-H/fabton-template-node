{
  "swagger": "2.0",
  "info": {
    "title": "Blockchain API Service",
    "description": "{{description}}",
    "version": "1.0.0",
    "contact": {
      "name": "{{author}}"
    }
  },
  "schemes": [
    "http"
  ],
  "basePath": "/api",
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "tags": [
    {{#each models}}
    {
      "name": "{{name}}",
      "description": "{{description}}"
    }
    {{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "paths": {
    {{#each models}}
    "/{{camelize name}}": {
      "post": {
        "tags": [
          "{{name}}"
        ],
        "summary": "create {{name}}",
        "description": "create {{name}}",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "{{name}} detail",
            "required": true,
            "schema": {
              "$ref": "#/definitions/{{name}}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/{{camelize name}}{{#each keys}}/{ {{~this~}} }{{/each}}": {
      "put": {
        "tags": [
          "{{name}}"
        ],
        "summary": "update {{name}}",
        "description": "update {{name}}",
        "parameters": [
          {{#each keys}}
          {
            "in": "path",
            "name": "{{this}}",
            "description": "{{this}}",
            "required": true,
            "type": "string"
          },
          {{/each}}
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/{{name}}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "get": {
        "tags": [
          "{{name}}"
        ],
        "summary": "select {{name}}",
        "description": "select {{name}}",
        "parameters": [
          {{#each keys}}
          {
            "in": "path",
            "name": "{{this}}",
            "description": "{{this}}",
            "required": true,
            "type": "string"
          }
            {{#unless @last}},{{/unless}}
          {{/each}}
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "delete": {
        "tags": [
          "{{name}}"
        ],
        "summary": "delete {{name}}",
        "description": "delete {{name}}",
        "parameters": [
          {{#each keys}}
          {
            "in": "path",
            "name": "{{this}}",
            "description": "{{this}}",
            "required": true,
            "type": "string"
          }
          {{#unless @last}},{{/unless}}
          {{/each}}
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    }{{#unless @last}},{{/unless}}
  {{/each}}
  },
  "definitions": {
    {{#each models}}
    "{{name}}": {
      "type": "object",
      "required": [
        {{#each keys}}
        "{{this}}"{{#unless @last}},{{/unless}}
        {{/each}}
      ],
      "properties": {
      {{#each properties}}
        "{{this}}": {
          "type": "string",
          "description": "{{this}}"
        }{{#unless @last}},{{/unless}}
          {{/each}}
      }
    },
    {{/each}}
    "Error": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}
